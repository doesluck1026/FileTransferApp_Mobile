<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="de" original="FILETRANSFERAPP_MOBILE/RESOURCES/APPRESOURCES.RESX" tool-id="MultilingualAppToolkit" product-name="n/a" product-version="n/a" build-num="n/a">
    <header>
      <tool tool-id="MultilingualAppToolkit" tool-name="Multilingual App Toolkit" tool-version="4.0.6916.0" tool-company="Microsoft" />
    </header>
    <body>
      <group id="FILETRANSFERAPP_MOBILE/RESOURCES/APPRESOURCES.RESX" datatype="resx">
        <trans-unit id="Ac_btnReceive" translate="yes" xml:space="preserve">
          <source>Receive Files</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Dateien empfangen</target>
        </trans-unit>
        <trans-unit id="Ac_btnSend" translate="yes" xml:space="preserve">
          <source>Send Files</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Dateien senden</target>
        </trans-unit>
        <trans-unit id="Ac_InfoMessage" translate="yes" xml:space="preserve">
          <source>Choose what to do.</source>
          <target state="translated">Wählen Sie, was zu tun ist.</target>
        </trans-unit>
        <trans-unit id="Done_btnHome" translate="yes" xml:space="preserve">
          <source>Home</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Startseite</target>
        </trans-unit>
        <trans-unit id="Done_btnOpen" translate="yes" xml:space="preserve">
          <source>Open</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Öffnen</target>
        </trans-unit>
        <trans-unit id="Done_InfoMessage" translate="yes" xml:space="preserve">
          <source>File transfer is succesful!</source>
          <target state="translated">Dateiübertragung ist erfolgreich!</target>
        </trans-unit>
        <trans-unit id="Done_lblFiles" translate="yes" xml:space="preserve">
          <source>Files</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Dateien</target>
        </trans-unit>
        <trans-unit id="Files_Warning_NoFile" translate="yes" xml:space="preserve">
          <source>No file is selected!</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Keine Datei ausgewählt.</target>
        </trans-unit>
        <trans-unit id="File_btnAdd" translate="yes" xml:space="preserve">
          <source>Add more</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Weitere hinzufügen</target>
        </trans-unit>
        <trans-unit id="File_btnConfirm" translate="yes" xml:space="preserve">
          <source>Confirm files</source>
          <target state="translated">Dateien bestätigen</target>
        </trans-unit>
        <trans-unit id="File_btnOpen" translate="yes" xml:space="preserve">
          <source>View</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Ansicht</target>
        </trans-unit>
        <trans-unit id="File_btnRemove" translate="yes" xml:space="preserve">
          <source>Remove</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Entfernen</target>
        </trans-unit>
        <trans-unit id="File_InfoMessage" translate="yes" xml:space="preserve">
          <source>Review selected files.</source>
          <target state="translated">Überprüfen Sie ausgewählte Dateien.</target>
        </trans-unit>
        <trans-unit id="File_SelectedList" translate="yes" xml:space="preserve">
          <source>List of selected files</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Liste der ausgewählten Dateien</target>
        </trans-unit>
        <trans-unit id="Info_InfoMessage" translate="yes" xml:space="preserve">
          <source>Thanks for using this application. You can follow and support us for more!</source>
          <target state="translated">Vielen Dank, dass Sie diese Anwendung verwenden. Sie können uns folgen und uns für mehr unterstützen!</target>
        </trans-unit>
        <trans-unit id="Main_btnSelect" translate="yes" xml:space="preserve">
          <source>Select FIles</source>
          <target state="translated">Dateien auswählen</target>
        </trans-unit>
        <trans-unit id="Main_InfoMessage1" translate="yes" xml:space="preserve">
          <source>Plase make sure that your phone and the receiver device are connected to same Wi-Fi!</source>
          <target state="translated">Bitte stellen Sie sicher, dass Ihr Telefon und das Empfängergerät mit demselben WLAN verbunden sind!</target>
        </trans-unit>
        <trans-unit id="Main_InfoMessage2" translate="yes" xml:space="preserve">
          <source>Select the files you want to send. You can select multiple files.</source>
          <target state="translated">Wählen Sie die Dateien aus, die Sie senden möchten. Sie können mehrere Dateien auswählen.</target>
        </trans-unit>
        <trans-unit id="Permission_btnAccept" translate="yes" xml:space="preserve">
          <source>Accept</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Annehmen</target>
        </trans-unit>
        <trans-unit id="Permission_InfoMessage" translate="yes" xml:space="preserve">
          <source>New Request</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Neue Anforderung</target>
        </trans-unit>
        <trans-unit id="Permission_RequestMessage" translate="yes" xml:space="preserve">
          <source>wants to send you some files : </source>
          <target state="translated">möchte Ihnen einige Dateien senden:</target>
        </trans-unit>
        <trans-unit id="Permission_RequestMessage2" translate="yes" xml:space="preserve">
          <source>Do you want to receive?</source>
          <target state="translated">Möchten Sie empfangen?</target>
        </trans-unit>
        <trans-unit id="Permisson_btnDecline" translate="yes" xml:space="preserve">
          <source>Decline</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Ablehnen</target>
        </trans-unit>
        <trans-unit id="Rec_InfoMessage" translate="yes" xml:space="preserve">
          <source>Waiting for connection...</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Auf Verbindung warten...</target>
        </trans-unit>
        <trans-unit id="Send_btnRefresh" translate="yes" xml:space="preserve">
          <source>Refresh</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Aktualisieren</target>
        </trans-unit>
        <trans-unit id="Send_btnSend" translate="yes" xml:space="preserve">
          <source>Send</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Senden</target>
        </trans-unit>
        <trans-unit id="Send_deviceList" translate="yes" xml:space="preserve">
          <source>Available device list :</source>
          <target state="translated">Liste der verfügbaren Geräte:</target>
        </trans-unit>
        <trans-unit id="Send_infoMessage" translate="yes" xml:space="preserve">
          <source>Select the target device</source>
          <target state="translated">Wählen Sie das Zielgerät aus</target>
        </trans-unit>
        <trans-unit id="Send_Scanning" translate="yes" xml:space="preserve">
          <source>Scanning Network...</source>
          <target state="translated">Scan-Netzwerk ...</target>
        </trans-unit>
        <trans-unit id="Send_TargetIP" translate="yes" xml:space="preserve">
          <source>Target IP :</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Ziel-IP</target>
        </trans-unit>
        <trans-unit id="Send_Warning_Goback" translate="yes" xml:space="preserve">
          <source>You can't go back after sending a request. Sorry...</source>
          <target state="translated">Sie können nach dem Senden einer Anfrage nicht mehr zurückkehren. Es tut uns leid...</target>
        </trans-unit>
        <trans-unit id="Send_Warning_rejected" translate="yes" xml:space="preserve">
          <source>Sorry... Transfer request is rejected by</source>
          <target state="translated">Entschuldigung ... Überweisungsanforderung wird von abgelehnt</target>
        </trans-unit>
        <trans-unit id="Settings_fristTimeInfo" translate="yes" xml:space="preserve">
          <source>It is better to change the name of your device so that you can easily recognize it later.
                           This name will be seen when you want to receive files. You can also change the language of this app</source>
          <target state="translated">Es ist besser, den Namen Ihres Geräts zu ändern, damit Sie es später leicht erkennen können.
                            Dieser Name wird angezeigt, wenn Sie Dateien empfangen möchten. Sie können auch die Sprache dieser App ändern</target>
        </trans-unit>
        <trans-unit id="Settings_WelcomeMessage" translate="yes" xml:space="preserve">
          <source>Welcome to Buggy File Transfer App!</source>
          <target state="translated">Willkommen bei der Buggy File Transfer App!</target>
        </trans-unit>
        <trans-unit id="Setting_btnSave" translate="yes" xml:space="preserve">
          <source>Save</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Speichern</target>
        </trans-unit>
        <trans-unit id="Setting_DeviceName" translate="yes" xml:space="preserve">
          <source>Device Name :</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Gerätename</target>
        </trans-unit>
        <trans-unit id="Setting_Language" translate="yes" xml:space="preserve">
          <source>Language :</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Sprache</target>
        </trans-unit>
        <trans-unit id="Setting_Warning_Saving" translate="yes" xml:space="preserve">
          <source>Saving Parameters...</source>
          <target state="translated">Parameter speichern ...</target>
        </trans-unit>
        <trans-unit id="Transfer_btnAbort" translate="yes" xml:space="preserve">
          <source>Cancel</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Abbrechen</target>
        </trans-unit>
        <trans-unit id="Transfer_ConfirmAbortMessage" translate="yes" xml:space="preserve">
          <source>Do you really want to cancel transfer?</source>
          <target state="translated">Möchten Sie die Übertragung wirklich abbrechen?</target>
        </trans-unit>
        <trans-unit id="Transfer_ConfirmAbortNo" translate="yes" xml:space="preserve">
          <source>No</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">NEIN</target>
        </trans-unit>
        <trans-unit id="Transfer_ConfirmAbortTitle" translate="yes" xml:space="preserve">
          <source>Confirmation</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Bestätigung</target>
        </trans-unit>
        <trans-unit id="Transfer_ConfirmAbortYes" translate="yes" xml:space="preserve">
          <source>Yes</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">JA</target>
        </trans-unit>
        <trans-unit id="Transfer_Filename" translate="yes" xml:space="preserve">
          <source>File name :</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Dateiname:</target>
        </trans-unit>
        <trans-unit id="Transfer_FileSize" translate="yes" xml:space="preserve">
          <source>File Size :</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Dateigröße:</target>
        </trans-unit>
        <trans-unit id="Transfer_InfoMessage" translate="yes" xml:space="preserve">
          <source>Files are being transferred...</source>
          <target state="translated">Dateien werden übertragen ...</target>
        </trans-unit>
        <trans-unit id="Transfer_Remaining" translate="yes" xml:space="preserve">
          <source>Remaining :</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">Verbleibend</target>
        </trans-unit>
        <trans-unit id="Transfer_Time" translate="yes" xml:space="preserve">
          <source>Time :</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">ZEIT</target>
        </trans-unit>
        <trans-unit id="Transfer_TransferSpeed" translate="yes" xml:space="preserve">
          <source>Transfer speed :</source>
          <target state="translated">Übertragungsgeschwindigkeit:</target>
        </trans-unit>
        <trans-unit id="Transfer_Warning_GoBack" translate="yes" xml:space="preserve">
          <source>You can't go back while files are being transferred!</source>
          <target state="needs-review-translation">Sie können beim Übertragen nicht zurückkehren!</target>
          <note from="MultilingualUpdate" annotates="source" priority="2">Please verify the translation’s accuracy as the source string was updated after it was translated.</note>
        </trans-unit>
        <trans-unit id="YourDeviceName" translate="yes" xml:space="preserve">
          <source>Your Device Name </source>
          <target state="needs-review-translation">Ihr Gerätename:</target>
          <note from="MultilingualUpdate" annotates="source" priority="2">Please verify the translation’s accuracy as the source string was updated after it was translated.</note>
        </trans-unit>
        <trans-unit id="YourIPAddress" translate="yes" xml:space="preserve">
          <source>Your IP Address</source>
          <target state="translated">Ihre IP-Adresse</target>
        </trans-unit>
        <trans-unit id="Transfer_AbortedMessage" translate="yes" xml:space="preserve">
          <source>Transfer is aborted by other device.</source>
          <target state="new">Transfer is aborted by other device.</target>
        </trans-unit>
      </group>
    </body>
  </file>
</xliff>